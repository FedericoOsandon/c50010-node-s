<div>
    <center>
        <h1>Users</h1>
        {{#each users}}
            <p><strong>Nombre: </strong>{{this.first_name}} {{this.last_name}}- <strong>Email: </strong>{{this.email}}</p>
        {{/each}}
        {{#if hasPrevPage}}
            <a href="users?pageQuery={{prevPage}}">Anterior</a>
        {{/if}}
        <label>{{page}}</label>
        {{#if hasNextPage}}
            <a href="/users?pageQuery={{nextPage}}">Siguiente</a>
        {{/if}}

    </center>
</div>
